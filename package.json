    "react-addons-test-utils": "^15.3.1",
    "redux-mock-store": "^1.1.4",
    "sinon": "^1.17.5",
    "sleep": "^3.0.1",
    "ts-loader": "^0.8.2",
    "ts-node": "^1.3.0",
    "typescript": "^1.8.10",
    "typings": "^1.3.2",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "axios": "^0.13.1",
    "object-assign": "^4.1.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
