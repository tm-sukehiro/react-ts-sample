{
  "name": "react-ts-sample",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "test:tsx": "mocha --require ts-node/register ./test-setup.es6 \"./**/__test__/**/*.tsx\"",
    "test:ts": "mocha --require ts-node/register \"./**/__test__/**/*.ts\"",
    "server": "node dev-server.es6",
